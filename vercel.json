{
  "outputDirectory": "./build",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.portal.grove.city"
        }
      ],
      "destination": "https://portal.grove.city/:path*",
      "permanent": true
    }
  ]
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow, googlebot: noindex, googlebot: nofollow"
        }
      ]
    }
  ]
}
